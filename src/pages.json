			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "XXX装修管理",
				"navigationBarBackgroundColor": "transparent"
			}
		},
		{
			"path": "pages/houseDetails/index",
			"style": {
				"navigationBarTitleText": "验房",
				"navigationBarBackgroundColor": "#FFFFFF"
			}
		},
		{
			"path": "pages/my/index",
			"style": {
